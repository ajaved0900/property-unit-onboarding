<h1>Property CSV Import</h1>

<%= form_with url: "/imports/preview",
              method: :post,
              multipart: true,
              data: { turbo: false } do %>

  <div style="margin-bottom: 15px;">
    <label for="file">CSV file:</label><br>
    <%= file_field_tag :file, accept: ".csv", required: true %>
  </div>

  <div>
    <%= submit_tag "Upload & Preview" %>
  </div>

<% end %>
